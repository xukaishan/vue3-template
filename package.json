{
  "name": "ks-vue3-template",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --mode localDevelopment --config ./build/vite.config.dev.js --open --host",
    "build": "vite build --config ./build/vite.config.build.js",
    "build:f": "npm run i && npm run build",
    "build:r": "vite build --config ./build/vite.config.build.js",
    "rm": "rimraf node_modules && rimraf .eslintcache && npm install",
    "i": "rimraf node_modules/@kscommon && rimraf package-lock.json && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "@kscommon/qz-ui": "latest",
    "@vueuse/components": "^9.1.0",
    "@vueuse/core": "^9.1.0",
    "jsencrypt": "^3.3.2",
    "pinia": "^2.0.22",
    "qs": "^6.11.0",
    "sass": "^1.54.4",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "rollup": "^2.78.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-visualizer": "^5.8.1",
    "terser": "^5.16.1",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-define-options": "^0.10.0",
    "vite": "^3.0.7",
    "vite-plugin-eslint": "^1.6.1",
    "vite-svg-loader": "^3.6.0",
    "vue-eslint-parser": "^9.0.2"
  }
}
